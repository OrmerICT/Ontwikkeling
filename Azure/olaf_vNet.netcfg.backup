<?xml version="1.0" encoding="utf-8"?>
<NetworkConfiguration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.microsoft.com/ServiceHosting/2011/07/NetworkConfiguration">
  <VirtualNetworkConfiguration>
    <Dns>
      <DnsServers>
        <DnsServer name="DUDO-DC-01" IPAddress="10.10.2.20" />
        <DnsServer name="TEST-DC-01" IPAddress="10.10.1.20" />
      </DnsServers>
    </Dns>
    <VirtualNetworkSites>
      <VirtualNetworkSite name="TEST-VNET-01" Location="West Europe">
        <AddressSpace>
          <AddressPrefix>10.10.1.0/24</AddressPrefix>
        </AddressSpace>
        <Subnets>
          <Subnet name="TEST-SUBNET-01">
            <AddressPrefix>10.10.1.0/25</AddressPrefix>
          </Subnet>
          <Subnet name="Gateway">
            <AddressPrefix>10.10.1.248/29</AddressPrefix>
          </Subnet>
        </Subnets>
        <DnsServersRef>
          <DnsServerRef name="TEST-DC-01" />
        </DnsServersRef>
      </VirtualNetworkSite>
    </VirtualNetworkSites>
  </VirtualNetworkConfiguration>
</NetworkConfiguration>
